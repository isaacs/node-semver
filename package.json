{
  "name": "semver",
  "version": "7.3.5",
  "description": "The semantic version parser used by npm.",
  "main": "index.js",
  "exports": {
    ".": {
      "require": "./index.js",
      "import": "./esm-wrapper.mjs"
    },
    "./bin/*": "./bin/*",
    "./classes/*": "./classes/*",
    "./functions/*": "./functions/*",
    "./internal/*": "./internal/*",
    "./ranges/*": "./ranges/*",
    "./bin/": "./bin/",
    "./classes/": "./classes/",
    "./functions/": "./functions/",
    "./internal/": "./internal/",
    "./ranges/": "./ranges/",
    "./index.js": "./index.js",
    "./index": "./index.js",
    "./preload.js": "./preload.js",
    "./preload": "./preload.js",
    "./package.json": "./package.json",
    "./package": "./package.json"
  },
  "scripts": {
    "test": "tap",
    "snap": "tap",
    "preversion": "npm test",
    "postversion": "npm publish",
    "postpublish": "git push origin --follow-tags"
  },
  "devDependencies": {
    "tap": "^14.10.7"
  },
  "license": "ISC",
  "repository": "https://github.com/npm/node-semver",
  "bin": {
    "semver": "bin/semver.js"
  },
  "files": [
    "bin/**/*.js",
    "range.bnf",
    "classes/**/*.js",
    "functions/**/*.js",
    "internal/**/*.js",
    "ranges/**/*.js",
    "index.js",
    "preload.js"
  ],
  "tap": {
    "check-coverage": true,
    "coverage-map": "map.js",
    "test-ignore": "test/esm-wrapper.mjs"
  },
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "lru-cache": "^6.0.0"
  }
}
